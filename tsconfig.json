{
	"compilerOptions": {
		"target": "es2020",
		"module": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"jsx": "react-jsx",
		"baseUrl": ".",
		"verbatimModuleSyntax": true,
		"noUncheckedIndexedAccess": true,
		"types": ["vite/client"]
	}
}
